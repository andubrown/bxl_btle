platform :ios, '15.5'

ENV['COCOAPODS_DISABLE_STATS'] = 'true'

use_frameworks!
use_modular_headers!

target 'Runner' do
  # Flutter manages pods automatically
  flutter_install_all_ios_pods(File.dirname(File.realpath(__FILE__)))
end

post_install do |installer|
  installer.pods_project.targets.each do |target|
    # Flutter handles build settings automatically, no need for undefined method
  end
end

